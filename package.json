{
  "name": "powerbuilder",
  "displayName": "PowerBuilder",
  "description": "Adds some PowerBuilder support",
  "version": "0.0.2",
  "publisher": "micha4w",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/micha4w/powerbuilder-vscode"
  },
  "bugs": {
    "url": "https://github.com/micha4w/powerbuilder-vscode/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "firstLine": "^((HA)?(?=\\$)|forward)",
        "id": "powerbuilder",
        "aliases": ["PowerBuilder", "pb"],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./icons/powerbuilder.png",
          "light": "./icons/powerbuilder.png"
        },
        "extensions": [
          "sru",
          "srd",
          "srm",
          "srw",
          "srs",
          "srf",
          "sra",
          "srp",
          "srj",
          "srq",
          "pbl",
          "pbt",
          "pbw"
        ]
      },
      {
        "id": "powerbuilder-codeblock"
      }
    ],
    "grammars": [
      {
        "language": "powerbuilder",
        "scopeName": "source.powerbuilder",
        "path": "./syntaxes/powerbuilder.json"
      },
      {
        "language": "powerbuilder-codeblock",
        "scopeName": "markdown.powerbuilder.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ]
  },
  "dependencies": {  }
}
